<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TutoMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TutoMaui"
    Shell.FlyoutBehavior="Disabled"
    Title="TutoMaui">
    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        <Tab Title="Test">
            <ShellContent
        Title="CollView"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
            <ShellContent
        Title="TableView"
        ContentTemplate="{DataTemplate local:AutrePage}"
        Route="AutrePage" />
            <ShellContent
        Title="Abs."
        ContentTemplate="{DataTemplate local:Absolute}"
        Route="Absolute" />
            <ShellContent
        Title="Flex"
        ContentTemplate="{DataTemplate local:Flex}"
        Route="Flex" />
            <ShellContent
        Title="Grid"
        ContentTemplate="{DataTemplate local:GridLayout}"
        Route="GridLayout" />
            
            
        </Tab>
    </FlyoutItem>
</Shell>
